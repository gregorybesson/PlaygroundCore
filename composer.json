{
    "name": "playground/core",
    "description": "Playground Core Module",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "zf2",
        "playground",
        "playground-core"
    ],
    "homepage": "https://github.com/gregorybesson/PlaygroundCore",
    "authors": [
        {
            "name": "Greg Besson",
            "email": "gregory.besson@playground.gg",
            "homepage": "http://www.playground.fr"
        }
    ],
    "require": {
        "php": ">=5.5",
		"zendframework/zendframework" : ">=2.0.0",
        "doctrine/doctrine-orm-module": ">=0.0.0",
        "gedmo/doctrine-extensions": ">=2.3.0",
        "beberlei/DoctrineExtensions": ">=1.0.0",
		"widmogrod/zf2-assetic-module": ">=1.0.0",
		"hounddog/doctrine-data-fixture-module": ">=0.0.0",
		"zendframework/zend-developer-tools" : ">=1.0.0",
		"kokspflanze/bjy-authorize": ">=1.6.0",
        "studio-42/elfinder": ">=2.0.0",
		"twilio/sdk" : ">=3.0.0",
		"buggedcom/phpvideotoolkit" : ">=2.0.0",
        "zfr/zfr-cors" : ">=1.0.0"
    },
	"require-dev" : {
		"phpunit/phpunit" : ">=4.0.0",
        "squizlabs/php_codesniffer": ">=2.0.0",
        "php-coverall/php-coveralls": ">=0.0.0"
	},
    "autoload" : {
		"psr-4" : {
			"PlaygroundCore\\" : "src/"
		}
	},
	"extra": {
        "branch-alias": {
            "dev-master": "4.x-dev"
        }
    }
}
